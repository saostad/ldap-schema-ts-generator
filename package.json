{
  "name": "ldap-schema-ts-generator",
  "version": "2.1.2",
  "description": "Typescript typedef and meta-data generator for LDAP Schema. It can be useful to interact from schema-aware/type-safe LDAP Client with LDAP servers like active directory.",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/ldap-schema-ts-generator.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --check \"src/**/*.ts\" --write",
    "lint": "eslint --ext \".ts,.js\" src --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "gen-docs": "typedoc",
    "postgen-docs": "node ./github/github.js",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "prepublishOnly": "npm run tsc",
    "prod": "node --unhandled-rejections=strict dist/app.js",
    "dev": "nodemon --watch dist --exec \"npm run prod\"",
    "start": "npm-run-all -p dev tsc:watch"
  },
  "keywords": [
    "ldap",
    "schema",
    "typescript",
    "interface",
    "type",
    "definition",
    "generator",
    "type",
    "active",
    "directory"
  ],
  "author": "Saeid Ostad",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1000"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^25.3.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/ldapjs": "^1.0.7",
    "@types/pino": "^5.17.0",
    "fast-node-logger": "^1.3.4",
    "ldap-ts-client": "0.0.3",
    "pascal-case": "^3.1.1",
    "prettier": "^2.0.4"
  }
}
